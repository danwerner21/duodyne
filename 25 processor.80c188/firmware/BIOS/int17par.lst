    33                                  	; BIOS_call_17h - Print services
    34                                  	;
    35                                  	; Purpose: INT 17h BIOS printer services.
    36                                  	; Entry : AH = 0 Print character in AL
    37                                  	; AH = 1 Initialize port
    38                                  	; AH = 2 Read printer port status to AH
    39                                  	;
    40                                  	; AL = character to print
    41                                  	; DX = 0 - based port number
    42                                  	; All registers preserved except AX
    43                                  	;
    44                                  	; Exit : AH = Port Status
    45                                  	; |7|6|5|4|3|2|1|0| AH (status)
    46                                  	; | | | | | | | ` - - - - time - out
    47                                  	; | | | | | ` - ' - - - - - - unused
    48                                  	; | | | | ` - - - - - - - - 1 = I / O error (pin 15)
    49                                  	; | | | ` - - - - - - - - - 1 = printer selected / on - line (pin 13)
    50                                  	; | | ` - - - - - - - - - - 1 = out of paper (pin 12)
    51                                  	; | ` - - - - - - - - - - - 1 = printer acknowledgment (pin 10)
    52                                  	; ` - - - - - - - - - - - - 1 = printer not busy (pin 11)
    53                                  	;
    54                                  	;===============================================================================
    55                                  	; ENTRY 0EFD2h ; IBM entry for parallel LPT
    56                                  
    57                                  	global BIOS_call_17h
    58                                  
    59                                  BIOS_call_17h:
    60 00000000 515352                  	pushm dx, bx, cx
    61 00000003 FB                      	sti                          ; Printer Services - Support Duodyne Media Board LPT
    62 00000004 80FC00                  	cmp ah, 0
    63 00000007 740E                    	je LP_OUT                    ; Function is print, AH=0
    64 00000009 80FC01                  	cmp ah, 1
    65 0000000C 7438                    	je LP_INI                    ; Function is init, AH=1
    66 0000000E 80FC02                  	cmp ah, 2
    67 00000011 7443                    	je LP_STS                    ; Get the status, AH=2
    68 00000013 5A5B59                  	popm dx, bx, cx
    69 00000016 CF                      	iret
    70                                  
    71                                  LP_OUT:
    72 00000017 BA4804                  	mov dx, ParPrinter
    73 0000001A EE                      	out dx, al                   ; Char - - > data lines 0 - 7
    74 0000001B BA4904                  	mov dx, ParPrinter + 1       ; Printer status port
    75 0000001E B720                    	mov bh, PrinterTimeout       ; Load time out parameter
    76 00000020 88C4                    	mov ah, al
    77                                  
    78                                  .1:
    79 00000022 31C9                    	xor cx, cx                   ; Clear lo order time out
    80                                  
    81                                  .2:
    82 00000024 EC                      	in al, dx                    ; Get line printer status
    83 00000025 08C0                    	or al, al                    ; ...ready?
    84 00000027 7812                    	js .3                        ; ...done if so
    85 00000029 E2F9                    	loop .2
    86 0000002B FECF                    	dec bh                       ; Decrement time out
    87 0000002D 75F3                    	jnz .1
    88                                  
    89 0000002F 0C01                    	or al, 00000001b             ; Set timeout in Status Byte
    90 00000031 24F9                    	and al, 11111001b            ; ...bits returned to caller
    91 00000033 3448                    	xor al, 01001000b            ; ...toggle ERROR, ACKNOWLEDGE
    92 00000035 86C4                    	xchg al, ah
    93                                  	; Exit, AH=Status, AL=character
    94 00000037 5A5B59                  	popm dx, bx, cx
    95 0000003A CF                      	iret
    96                                  
    97                                  .3:
    98 0000003B BA4A04                  	mov dx, ParPrinter + 2       ; Printer control port
    99 0000003E B00D                    	mov al, 00001101b            ; Set output strobe hi
   100 00000040 EE                      	out dx, al                   ; ...data lines 0 - 7 valid
   101 00000041 B00C                    	mov al, 00001100b            ; Set output strobe lo
   102 00000043 EE                      	out dx, al                   ; ...data lines 0 - 7 ?????
   103 00000044 EB10                            jmp LP_STS
   104                                  
   105                                  LP_INI:
   106 00000046 88C4                            mov ah,al                ; Initialize the line printer
   107 00000048 BA4A04                  	mov dx, ParPrinter + 2       ; Printer control port
   108 0000004B B008                    	mov al, 00001000b
   109 0000004D EE                      	out dx, al                   ; Request initialize
   110                                  
   111 0000004E B9DC05                  	mov cx, 5DCh                 ; ...delay
   112                                  .4:
   113 00000051 E2FE                            loop .4
   114 00000053 B00C                    	mov al, 00001100b            ; Set output strobe lo
   115 00000055 EE                      	out dx, al                   ; ...data lines 0 - 7 ?????
   116                                  LP_STS:
   117 00000056 BA4904                  	mov dx, ParPrinter + 1       ; Printer status port
   118 00000059 EC                      	in al, dx                    ; Read printer status
